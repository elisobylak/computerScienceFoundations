import java.util.*;
import java.util.ArrayList;

public class FlightList {
  
  private ArrayList<FlightInfo> list;
  
  public void FlighList()
  {
    list = new ArrayList<FlightInfo>();
  }
  
  public void addFlightInfo(FlightInfo newFlyer)
  {
    list.add(newFlyer);
  }
  
  public void printList()
  {
    for (int i = 0; i < list.size(); i++)
    {
      System.out.println(list.get(i).toString());
    }
  }
  
  public static void main(String[] args)
  {
    FlightList listOne = new FlightList();
    FlightInfo flyerOne = new FlightInfo();
    flyerOne.setName();
    flyerOne.setDestination();
    flyerOne.setSeat();
    
    System.out.println(flyerOne);
    
    
    /*listOne.addFlightInfo(flyerOne);*/
    
  }
}






